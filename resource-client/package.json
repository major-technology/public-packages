{
  "name": "@major-tech/resource-client",
  "version": "0.2.8",
  "description": "TypeScript client library for invoking Major resources (PostgreSQL, Custom APIs, HubSpot, S3)",
  "type": "module",
  "sideEffects": false,
  "engines": {
    "node": ">=18.0.0"
  },
  "bin": {
    "major-client": "./bin/generate-clients.mjs"
  },
  "files": [
    "dist",
    "bin",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "pnpm clean && tsc && node scripts/build.mjs",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm run typecheck && pnpm run build"
  },
  "keywords": [
    "major",
    "resource",
    "client"
  ],
  "author": "Major Technology",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/major-technology/public-packages.git",
    "directory": "vite-resource-client"
  },
  "homepage": "https://github.com/major-technology/public-packages/tree/main/vite-resource-client#readme",
  "bugs": {
    "url": "https://github.com/major-technology/public-packages/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "esbuild": "^0.24.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.935.0"
  }
}
